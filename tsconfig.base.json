      "@flexsuite/core/enums": ["libs/core/src/lib/enums/index.ts"],
      "@flexsuite/core/icons": ["libs/core/src/lib/icons/index.ts"],
      "@flexsuite/core/interfaces": ["libs/core/src/lib/interfaces/index.ts"],

      "analy/Routes": ["modules/analy/src/app/remote-entry/entry.routes.ts"],
      "atend/Routes": ["modules/atend/src/app/remote-entry/entry.routes.ts"],
      "conti/Routes": ["modules/conti/src/app/remote-entry/entry.routes.ts"],
      "fatur/Routes": ["modules/fatur/src/app/remote-entry/entry.routes.ts"],
      "logis/Routes": ["modules/logis/src/app/remote-entry/entry.routes.ts"],
      "produ/Routes": ["modules/produ/src/app/remote-entry/entry.routes.ts"],
      "rehum/Routes": ["modules/rehum/src/app/remote-entry/entry.routes.ts"],
      "secas/Routes": ["modules/secas/src/app/remote-entry/entry.routes.ts"],
      "supri/Routes": ["modules/supri/src/app/remote-entry/entry.routes.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
